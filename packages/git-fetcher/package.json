    "@pnpm/prepare-package": "workspace:*",
    "@zkochan/rimraf": "^2.1.2",
    "execa": "npm:safe-execa@^0.1.1"
  },
  "devDependencies": {
    "@pnpm/cafs": "workspace:*",
    "@pnpm/git-fetcher": "workspace:*",
    "@pnpm/package-store": "workspace:*",
