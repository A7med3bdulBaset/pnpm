  },
  "homepage": "https://github.com/pnpm/pnpm/blob/master/packages/plugin-commands-audit#readme",
  "devDependencies": {
    "@pnpm/types": "workspace:6.4.0",
    "@types/ramda": "0.27.39",
    "@types/zkochan__table": "npm:@types/table@^5.0.0",
    "nock": "12.0.3",
    "strip-ansi": "^6.0.0"
  },
  "dependencies": {
    "@pnpm/audit": "workspace:1.1.24",
    "@pnpm/cli-utils": "workspace:0.5.4",
    "@pnpm/config": "workspace:11.14.2",
    "@pnpm/constants": "workspace:4.1.0",
    "@pnpm/error": "workspace:1.4.0",
    "@pnpm/lockfile-file": "workspace:3.2.1",
    "@zkochan/table": "^1.0.0",
    "chalk": "^4.1.2",
    "ramda": "^0.27.1",
    "render-help": "^1.0.2"
