{
  "name": "@pnpm/plugin-commands-audit",
  "version": "3.0.6",
  "description": "pnpm commands for dependencies audit",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
