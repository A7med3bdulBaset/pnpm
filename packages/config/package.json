{
  "name": "@pnpm/config",
  "version": "13.14.2",
  "description": "Gets configuration options for pnpm",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "scripts": {
