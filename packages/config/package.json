  "dependencies": {
    "@pnpm/constants": "workspace:*",
    "@pnpm/error": "workspace:*",
    "@pnpm/global-bin-dir": "workspace:*",
    "@pnpm/npm-conf": "1.0.4",
    "@pnpm/pnpmfile": "workspace:*",
    "@pnpm/read-project-manifest": "workspace:*",
    "@pnpm/types": "workspace:*",
    "camelcase": "^6.3.0",
    "can-write-to-dir": "^1.1.1",
    "is-subdir": "^1.2.0",
